{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
    },
    "strict": true,
    "sourceMap": false,
    "target": "ES6",
    "module": "CommonJS",
    "lib": ["ES2020", "DOM"],
    "esModuleInterop": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true
  },
  "include": ["./src/**/*", "sample/metabase-bigquery-transfer.ts"],
  "exclude": ["node_modules"]
}
